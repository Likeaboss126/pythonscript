fetch("https://<controller-domain>/controller/rest/applications?output=JSON", {
  headers: { "Authorization": "Bearer <your-access-token>" }
})
.then(res => res.text())
.then(console.log)
.catch(console.error);


curl -X GET "https://<controller-domain>/controller/rest/applications?output=JSON" ^
     -H "Authorization: Bearer <your-access-token>"
